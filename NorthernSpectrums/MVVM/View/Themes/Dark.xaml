<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <DataObject x:Key="ApplicationTheme"/>
    <SolidColorBrush Color="#141414" x:Key="Background"/>
    <SolidColorBrush Color="#242424" x:Key="Tab"/>
    <SolidColorBrush Color="#1c1c1c" x:Key="TabButtonHover"/>
    <SolidColorBrush Color="#262626" x:Key="Dropdown"/>
    <SolidColorBrush Color="#666666" x:Key="DropdownBorder"/>
    <SolidColorBrush Color="#424242" x:Key="DropdownSelected"/>
    <SolidColorBrush Color="#ffffff" x:Key="Foreground"/>
</ResourceDictionary>